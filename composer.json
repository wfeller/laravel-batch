{
    "name": "wfeller/laravel-batch",
    "description": "Insert, update or delete models in batch, while still firing model events.",
    "keywords": ["laravel", "batch", "queue"],
    "homepage": "https://github.com/wfeller/laravel-batch",
    "license": "MIT",
    "authors": [
        {
            "name": "William",
            "homepage": "https://github.com/wfeller"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/bus": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13.3|^3.1.4",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "WF\\Batch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WF\\Batch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
